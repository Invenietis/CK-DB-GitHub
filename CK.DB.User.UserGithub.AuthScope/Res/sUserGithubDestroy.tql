create transformer on CK.sUserGithubDestroy
as
begin
	inject "
	declare @ScopeSetId int;
	select @ScopeSetId = ScopeSetId from CK.tUserGithub where UserId = @UserId;" 
	into "PreDestroy";
	
	inject "exec CK.sAuthScopeSetDestroy @ActorId, @ScopeSetId;" 
	into "PostDestroy";
end
